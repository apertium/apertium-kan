    <e lm="ಮನೆ">          <i>ಮನೆ</i><par n="ಮನೆ__n"/></e>
    <e lm="ಉದ್ಯಾನ">      <i>ಉದ್ಯಾನ</i><par n="ಮನೆ__n"/></e>   
    <e lm="ಹವಾಮಾನ">      <i>ಹವಾಮಾನ</i><par n="ಹವಾಮಾನ__n"/></e>
