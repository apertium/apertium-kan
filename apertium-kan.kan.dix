    <e lm="ಸಹೋದರ">        <i>ಸಹೋದರ</i><par n="ಸಹೋದರ__n"/></e> 
    <e lm="ವರ್ಷ">        <i>ವರ್ಷ</i><par n="ಮನೆ__n"/></e>
